{
  "name": "@traduki/rollup-plugin-traduki",
  "version": "0.9.1",
  "description": "Rollup plugin for Traduki",
  "author": "havelaer",
  "keywords": [
    "rollup-plugin",
    "messageformat",
    "pluralformat",
    "icu",
    "i18n",
    "L10n",
    "code-splitting",
    "translations"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "publishConfig": {
