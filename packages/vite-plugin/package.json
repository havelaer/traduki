{
  "name": "@traduki/vite-plugin-traduki",
  "version": "0.10.1",
  "description": "Vite plugin for Traduki",
  "author": "havelaer",
  "keywords": [
    "vite-plugin",
    "messageformat",
    "pluralformat",
    "icu",
    "i18n",
    "L10n",
    "code-splitting",
    "translations"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "publishConfig": {
