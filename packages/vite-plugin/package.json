  },
  "devDependencies": {
    "@traduki/runtime": "^0.10.0",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "vite": "^2.4.4"
  },
  "gitHead": "49bf5e7fed29f7eacb21349555d5a195ff410a5e"
